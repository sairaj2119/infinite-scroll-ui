datasource db {
  provider = "mongodb"
  url      = env("MONGODB_ATLAS")
}

generator client {
  provider = "prisma-client-js"
  previewFeatures=["mongoDb"]
}


model Post {
  id         String        @id @default(dbgenerated()) @map("_id") @db.ObjectId

  title      String
  createdAt  DateTime   @default(now())
}
